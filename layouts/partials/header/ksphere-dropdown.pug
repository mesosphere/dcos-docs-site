-
  var ksphereMenu = [{
    "title": "Ksphere",
    "children": [
      {
        "title": "Konvoy",
        "path":"/ksphere/konvoy"
      }, {
        "title": "Partner Solutions",
        "path":"/ksphere/konvoy/partner-solutions"
      }, {
        "title": "Kommander",
        "path":"/ksphere/kommander"
      }, {
        "title": "Dispatch",
        "path":"/ksphere/dispatch"
      }, {
        "title": "Kaptain",
        "path":"/ksphere/kaptain"
      }, {
        "title": "Conductor",
        "path":"/ksphere/conductor"
      }
    ]
  }]

each sphere in ksphereMenu
  #ks.chooser
    .chooser-current
      a.chooser-title!=sphere.title
      svg(class="chooser-svg" id='kspherer-svg')
        path(class="pointer" d="m 13,6 -5,5 -5,-5 z", fill="#858585")
    ul.chooser-list#kspherer-list
      each section in sphere.children
        li.chooser-list-item
          a(href=section.path)!= section.title
