    "mustache": "^4.1.0",
    "node-sass": "npm:sass@^1.49.9",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "engines": {
    "node": "^16.0.0",
    "npm": ">=7.0.0"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.md": [
      "vale --config=./.vale.ini",
