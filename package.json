    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "debug": "^3.1.0",
    "eslint": "^8.3.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "husky": "7.0.4",
    "js-yaml": "^3.9.1",
    "lint-staged": "^12.1.2",
    "markdown-it": "^12.0.4",
    "markdown-it-anchor": "^4.0.0",
    "markdown-it-attrs": "^1.2.1",
