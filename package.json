    "remark-lint-no-dead-urls": "1.1.0",
    "remark-validate-links": "11.0.2",
    "sanitize-html": "^1.14.1",
    "sass-loader": "^6.0.6",
    "semver": "^7.3.2",
    "semver-sort": "0.0.4",
    "shortcode-parser": "0.0.1",
